{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "jest": true
  },
  "plugins": ["prettier", "jest"],
  "extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "indent": "error",
    "no-multiple-empty-lines": "error",
    "no-unused-vars": 1
  }
}
