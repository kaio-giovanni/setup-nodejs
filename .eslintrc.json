{
  "plugins": ["prettier", "jest"],
  "extends": ["airbnb-base", "plugin:jest/recommended", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "indent": "error",
    "no-multiple-empty-lines": "error",
    "no-unused-vars": 1
  }
}
